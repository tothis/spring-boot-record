package base_package

import com.example.entity.User

function void out(String str) {
    System.out.println(str);
}

rule 未成年
    when
        $message:User(age < 18)
    then
        out("未成年");
        $message.setName("李磊");
end

rule 已成年
    when
        $message:User(age >= 18)
    then
        out("已成年");
        $message.setName("frank");
end